#include <bits/stdc++.h>
using namespace std;

#define INF 100000000
#define ll long long
#define REP(i,n) for(int i=0;i<n;i++)
#define REPP(i,m,n) for(int i=m;i<n;i++)
#define de cout << "debug" << endl;

int main() {
  ll H,W,N,Sr,Sc;
  cin >> H >> W >> N >> Sr >> Sc;
  string S,T;
  cin >> S >> T;
  
  //累積和的に考える
  ll migi = Sc;
  ll hida = Sc;
  ll ue = Sr;
  ll sita = Sr;
  
  REP(i,N){
    if(S[i] == 'R'){
      migi += 1;
      if(migi > W){
        cout << "NO" << endl;
        return 0;
      }
    }
    else if(S[i] == 'L'){
      hida -= 1;
      if(hida < 1){
        cout << "NO" << endl;
        return 0;
      }
    }
    else if(S[i] == 'U'){
      ue -= 1;
      if(ue < 1){
        cout << "NO" << endl;
        return 0;
      }
    }
    else if(S[i] == 'D'){
      sita += 1;
      if(sita > H){
        cout << "NO" << endl;
        return 0;
      }
    }
    
    if(T[i] == 'L'){
      migi -= 1;
      if(migi < 1){
        migi += 1;
      }
    }
    else if(T[i] == 'R'){
      hida += 1;
      if(hida > W){
        hida -= 1;
      }
    }
    else if(T[i] == 'D'){
      ue += 1;
      if(ue > H){
        ue -= 1;
      }
    }
    else if(T[i] == 'U'){
      sita -= 1;
      if(sita < 1){
        sita += 1;
      }
    }
    
  }
  
  cout << "YES" << endl;
}
